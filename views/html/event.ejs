            <td><input type="button" value="double click" ondblclick="function8()"></td>  
        </tr>
        <tr>
            <td>onKeyDown</td>
            <td><input type="text" name="" id="keydown" onkeydown="function9()"></td>
            <td>onKeyUp</td>
            <td><input type="text" name="keyup" id="keyup" onkeyup="function10()"></td>
            <td>onKeyPress</td>
            <td><input type="text" name="keypress" id="keypress" onkeypress="function11()"></td>   
        </tr>
        <tr>
            <td>onfocus</td>
