            <td>onUnload</td>
            <td><h4 id="unload"></h4></td>
            <td>onresize</td>
            <td><h4 id="size"></h4></td>   
        </tr>
        <tr>
            <td>oncopy</td>
            <td><h4 id="copy" oncopy="function18()">copy this text</h4></td>
            <td>ondrag</td>
            <td><h4 id="drag" ondrag="function19()">drag this text</h4></td>
            <td>ondrop</td>
            <td><h4 id="drop" ondrag="function20()">drag and drop this text</h4></td>
        </tr>
        <tr>
            <td>oninput</td>
            <td><input type="text" name="" id="input" oninput="function21()"></td>
