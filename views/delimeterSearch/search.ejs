<%- include('header') %>
<%- include('main') %>
<table border="1" align="center">
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Semester</th>
        <th>Gender</th>
        <th>Dob</th>
        <th>No</th>
        <th>city</th>
        <th>address</th>
        <th>zipcode</th>
        <th>B_group</th>
    </tr> 
  
    <% result.forEach( (row) => { %>
    <tr>
            <td><%= row.id  %></td>
            <td><%= row.f_name %></td>
            <td><%= row.l_name  %></td>
            <td><%= row.email  %></td>
            <td><%= row.semester  %></td>
            <td><%= row.gender  %></td>
            <td><%= row.dob  %></td>
            <td><%= row.No  %></td>
            <td><%= row.city  %></td>
            <td><%= row.address  %></td>
            <td><%= row.zipcode  %></td>
            <td><%= row.B_group %></td>
        </tr>
     <% }) %>
</table>
<%- include('footer') %>
